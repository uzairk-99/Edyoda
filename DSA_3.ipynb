   "source": [
    "## 2. Find height of a given tree"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "id": "842cf033",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining function for finding height\n",
    "def height(self):\n",
    "    if self is None:\n",
    "        return -1 \n",
    "    else:\n",
    "        lDepth = height(self.left)\n",
    "        rDepth = height(self.right)\n",
    "        \n",
    "        if lDepth > rDepth:\n",
    "            return lDepth+1\n",
    "        else:\n",
    "            return rDepth+1"
   ]
  },
